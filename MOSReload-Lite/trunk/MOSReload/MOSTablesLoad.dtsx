        LEFT JOIN [TrafficData].bv.SYSLOOKUP ARLookup ON ARLookup.SLU_ID = dea.DEA_SLU_ID_ASPECTRATIO
		left join [TrafficData].[PBS].[MasterLookupValues] mlvAspectRation on mlvAspectRation.bv_value = ARLookup.SLU_DESC
			AND mlvAspectRation.ML_ID = 13
			and mlvAspectRation.field = 'ASPECTRATIO_LOOKUP'
        LEFT JOIN [TrafficData].[PBS].[MasterLookupValues] mlvISPBSMINORITYPRODUCTION ON CAST(mlvISPBSMINORITYPRODUCTION.bv_value AS INT) = dea.DEA_ISPBSMINORITYPRODUCTION
                                                              AND mlvISPBSMINORITYPRODUCTION.field = 'trafficdata.BV.DEAL.DEA_ISPBSMINORITYPRODUCTION'
