                  contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
                  description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
                  name="ADO NET Source"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select   Q.ass_id
              , Q.[Episode.ContentIdentifier]
              , Q.[Episode.UID]
              , Q.[Episode.ParentID]
              , Q.[Episode.Referent.Type]
              , Q.[Episode.Title120]
              , Q.[Episode.TitleClass]
              , Q.[Episode.EpisodeNumber]
              , Q.[Episode.Title]
              , Q.[Episode.EpisodeClass]
              , Q.[Episode.SynopsisShort]
              , Q.[Episode.SynopsisLong]
              , Q.[Episode.CountryOfOrigin]
              , Q.[Episode.PrimaryGenre]
              , Q.[Episode.SecondaryGenre]
              , Q.[Episode.Distribution.Number]
              , Q.[Episode.ReleaseDate]
              , Q.[Episode.ApproximateLength]
              , Q.[Episode.Language.Primary.mode]
              , Q.[Episode.LanguagePrimary]
              , Q.[Episode.LanguageSecondary]
              , Q.DealID
from
(
select distinct a.[ass_id]
      ,[Episode.ContentIdentifier]
      ,[EMMEpisode.UID] as [Episode.UID]
      ,[EMMEpisode.ParentID] as [Episode.ParentID]
      ,[Episode.Referent.Type]
      ,[Episode.Title120]
      ,[Episode.TitleClass]
      ,[Episode.EpisodeNumber]
	  ,[Episode.Title]
	  ,case 
			when p.ASS_ID is not null then 
				--Q.episodeclass
			        case Q.EXT_INTEGER
				when 609587740 then 'one-time-only'
				when 609587737 then 'Main'
				when 619411205 then 'Special'
				when 619411780 then 'Standalone'
			        else 
                                                                          null
                                                               end
			else 
				case d.ACA_DESC
					when 'Episode in a Series' then 'Standalone'
					when 'One-time-only' then
						case 
							when upper([Episode.Title]) like '%SPECIAL%' then 'Special'
							when  upper([Episode.Title]) not like '%SPECIAL%' then 'one-time-only'

						end
					when 'Series' then 'Main'
					else NULL
				end
		end as [Episode.EpisodeClass]

      ,[Episode.SynopsisShort]
      ,[Episode.SynopsisLong]
      ,[Episode.CountryOfOrigin]
      ,[Episode.PrimaryGenre]
      ,[Episode.SecondaryGenre]
      ,[Episode.Distribution.Number]
      ,CONVERT(NVARCHAR(10), [Episode.ReleaseDate],126) as [Episode.ReleaseDate]
      ,[Episode.ApproximateLength]
      ,[Episode.Language.Primary.mode]
      ,[Episode.LanguagePrimary]
      ,[Episode.LanguageSecondary]
      ,[DealID]
  FROM [PBS].vwMOSEpisodeContactAgnostic a with (nolock) 
join	bv.ASSET b  with (nolock)  on b.ass_id = a.ass_id
join	[BV].[PBSFEEDDISTRIBUTOR] c  with (nolock)  on c.PBSFDS_ID = b.ASS_PBSFD_ID
			and c.PBSFDS_ISPBSDISTRIBUTOR = 1
join	bv.ASSETCATEGORY d  with (nolock)  on d.ACA_ID = b.ASS_ACA_ID
left join
	(
		select BV.ASSET.ASS_PBSNOLAROOT_CM        as NOLARoot
			 , BV.EXTENDEDDATA.EXT_OWNERRECORD_ID as ASS_ID
			 , BV.EXTENDEDDATA.EXT_INTEGER        as CleansedbyProgramming
			 , BV.ASSET.ASS_EPISODESLATE
		from BV.EXTENDEDDATA with (nolock)
			left outer join BV.ASSET with (nolock)
				on BV.EXTENDEDDATA.EXT_OWNERRECORD_ID = BV.ASSET.ASS_ID
		where (BV.EXTENDEDDATA.EXT_INTEGER = 652154224)
			  and (BV.EXTENDEDDATA.P_DELETEFLAG = 0)
			  and (BV.EXTENDEDDATA.EXT_UPDATEUSERID is not null)

	) P on p.ASS_ID = b.ASS_ID
left outer join 
	(
		SELECT bb.ULU_DESC AS episodeclass,aa.EXT_OWNERRECORD_ID,aa.EXT_INTEGER
		FROM BV.EXTENDEDDATA aa with (nolock)
		left OUTER join BV.USERLOOKUP bb with (nolock) on aa.EXT_INTEGER = bb.ULU_ID
		WHERE (aa.EXT_INTEGER in ( 609587740,609587737,619411205,619411780))
	) Q on Q.EXT_OWNERRECORD_ID = a.ass_id
) Q


