            WHERE ([TrafficData].BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871743)
        ) P ON P.EXT_OWNERRECORD_ID = ass.ASS_ID    
		 left join [TrafficData].bv.language subLan on sublan.lan_id = dea.DEA_LAN_ID_SUBTITLES
