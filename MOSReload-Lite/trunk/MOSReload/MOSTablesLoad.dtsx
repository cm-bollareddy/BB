                  CHARINDEX('NOLACode', [Episode.ContentIdentifier]) + 9, 4) AS NolaRoot ,
        replace([Episode.ContentIdentifier],'cid:org:pbs.org:NOLACode:','') AS Nolacode,
        mlvAssIsPublished.rl_value AS entity_status ,
        mlvWorkflowStatus.rl_value AS WorkflowStatus ,
        CASE [Episode.TitleClass]
