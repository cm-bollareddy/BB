                      <properties>
                        <property
                          dataType="System.String"
                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select	contract_sid
		,Contract_ID AS ContractID ,
        IP_Rights AS [Rights_ID] ,
        Assets AS Assets ,
        Right_Type AS RightType ,
        Content_Types AS ContentType ,
        Exclusivity AS Exclusivity ,
        Comment AS Comment ,
        actual_start_datetime AS ActualStartDate ,
        actual_end_datetime AS ActualEndDate ,
        Territories AS Territory ,
        Languages ,
        Sublicense_Flag
FROM    [z.uv_Contract_Rights]
WHERE   right_Type IN ( 'License' )
        AND Content_Types NOT IN ( 'Merchandising', 'Publishing (incl. eBook)',
                                   'Music Rights', 'Theatrical',
                                   'Audiostreaming',
                                   'Learning Media (LM) Rights - All Levels',
                                   'Learning Media (LM) Rights - Level 0', 
								   'Learning Media (LM) Rights - Level 1', 'Learning Media (LM) Rights - Level 2',
								   'Learning Media (LM) Rights - Level 3 (A+B)', 'Clip', 'Radio Origination',
                                   'Common Carrier' )
        AND [Territories] NOT IN ( 'Worldwide (Ex US, terr, &amp; possessions)',
                                   'Worldwide excl. Canada',
                                   'Worldwide ex U.S.',
                                   'Worldwide excl. United States and Canada',
                                   'Worldwide ex U.S., Terr., Canada, Caribbean',
                                   'U.S., Territories, Consulates, Bases, Ships and Canada',
                                   'Worldwide ex U.S., Terr., Consul., Bases, Ships, Canada',
