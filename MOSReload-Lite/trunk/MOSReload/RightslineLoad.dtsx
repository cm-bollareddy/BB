                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT     
	c.contract_id AS [pbs_alliant_id], 
	c.contract_description AS [entity_title],
    c.contract_comment AS [overview_notes], 
	MAX(CASE WHEN cu.udf_sid = '225' THEN cu.udf_value END) AS [Master Title], 
	MAX(CASE WHEN cu.udf_sid = 230 AND cu.udf_value = 132 THEN 'Executed' WHEN cu.udf_sid = 230 AND cu.udf_value = 133 THEN 'Unexecuted' END) AS [executed_status], 
    MAX(CASE WHEN cu.udf_sid = '231' THEN cu.udf_value END) AS [effective_date],                                             
