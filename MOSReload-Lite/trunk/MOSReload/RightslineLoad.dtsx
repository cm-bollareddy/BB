select	distinct 'MasterOffer' + cast(d.MDL_ID as nvarchar(100)) as Item_BV_ID,
		B.FranchiseName + ' (Season ' + cast(B.SeasonNumber as nvarchar(100)) + ')' as entity_title,
		c.DEA_ALLIANTCONTRACTID as pbs_alliant_id,
		cast(e.Programming_service as  nvarchar) as Programming_service  ,
		cast(e.RightsAndRestrictionsNotes as  nvarchar) as RightsAndRestrictionsNotes,
		cast(e.Executed_Status as  nvarchar) as Executed_Status,
		e.financial_notes,
		cast('No' as nvarchar) as PBSCanSecureUnderWriting,
			 mlvPBSD_Label.rl_value as entity_status
	 ,mlvStaticSTCon.rl_value as   standard_terms__conditions
	 ,cast(mlvWorkflowStatus.rl_value as int) as WorkflowStatus
from	bv.DEALCONTENT A
join
(
SELECT  R.AssetID,Q.FranchiseName, R.SeasonNumber, S.IsCleansed
FROM  
	--Franchise Info
	( SELECT    BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID],BV.USERLOOKUP.ULU_DESC As FranchiseName
        FROM      BV.EXTENDEDDATA
                LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
        WHERE     ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871743 )
    ) Q
    JOIN 
	--Original Season Info
	( SELECT   BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID], bv.EXTENDEDDATA.EXT_INTEGER as SeasonNumber
            FROM     BV.EXTENDEDDATA
                    LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
            WHERE    ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871751 )
			and		bv.EXTENDEDDATA.EXT_INTEGER &gt; 0
    ) R ON r.AssetID = q.AssetID
	
    left outer JOIN 
	--Programming Cleansed Data
	( SELECT   bv.EXTENDEDDATA.EXT_INTEGER as IsCleansed, BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID]
            FROM     BV.EXTENDEDDATA
                    LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
            WHERE    ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 622534631 )
			and		bv.EXTENDEDDATA.EXT_INTEGER != 1
    ) S ON S.AssetID = R.AssetID
	
) B on a.DCO_ASS_ID = b.AssetID
join bv.DEAL C on c.DEA_ID = a.DCO_DEA_ID
join bv.MASTERDEAL D on d.MDL_ID = c.DEA_MDL_ID
join pbs.DealPBSPAA1 E on e.dea_id = c.DEA_ID
join cteDealQuery  F on f.dea_id =  e.dea_id
  left JOIN trafficdata.BV.PBSDEALSTATUS pbsds ON pbsds.PBSDS_ID = c.DEA_PBSDS_ID
  left join [PBS].[MasterLookupValues] mlvPBSD_Label on mlvPBSD_Label.bv_value = pbsds.PBSDS_LABEL
				and mlvPBSD_Label.ml_id = 1
