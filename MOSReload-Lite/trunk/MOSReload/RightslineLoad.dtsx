                    <component
                      refId="Package\Sequence Container 1\Deal Imports\Import Deal-PBS Prod and Acq Merged\MOS-DealPbs ProdAcq Merged Data"
                      componentClassID="{165A526D-D5DE-47FF-96A6-F8274C19826B}"
                      contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                      description="OLE DB Source"
                      name="MOS-DealPbs ProdAcq Merged Data"
                      usesDispositions="true"
                      version="7">
                      <properties>
                        <property
                          dataType="System.Int32"
                          description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                          name="CommandTimeout">0</property>
                        <property
                          dataType="System.String"
                          description="Specifies the name of the database object used to open a rowset."
                          name="OpenRowset">[PBS].[DealPBSPAA1]</property>
                        <property
                          dataType="System.String"
                          description="Specifies the variable that contains the name of the database object used to open a rowset."
                          name="OpenRowsetVariable"></property>
                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT	DISTINCT
        'MasterOffer' + CAST(d.MDL_ID AS NVARCHAR(100)) AS Item_BV_ID ,
        B.FranchiseName + ' (Season ' + CAST(B.SeasonNumber AS NVARCHAR(100))
        + ')' AS entity_title ,
        c.DEA_ALLIANTCONTRACTID AS pbs_alliant_id ,
        e.Programming_service ,
        mlvPBSD_Label.rl_value AS entity_status ,
        CAST(mlvWorkflowStatus.rl_value AS INT) AS WorkflowStatus ,
        CAST('No' AS NVARCHAR) AS [PBSCanSecureUnderWriting] ,
        0 AS fee ,
        0 AS total_budget ,
        0 AS cpb_funding ,
        0 AS total_pbs_fees
FROM    bv.DEALCONTENT A
        JOIN ( SELECT   R.AssetID ,
                        Q.FranchiseName ,
                        R.SeasonNumber ,
                        S.IsCleansed
               FROM     --Franchise Info
                        ( SELECT    BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID] ,
                                    BV.USERLOOKUP.ULU_DESC AS FranchiseName
                          FROM      BV.EXTENDEDDATA
                                    LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
                          WHERE     ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871743 )
                        ) Q
                        JOIN --Original Season Info
                        ( SELECT    BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID] ,
                                    bv.EXTENDEDDATA.EXT_INTEGER AS SeasonNumber
                          FROM      BV.EXTENDEDDATA
                                    LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
                          WHERE     ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871751 )
                                    AND bv.EXTENDEDDATA.EXT_INTEGER &gt; 0
                        ) R ON r.AssetID = q.AssetID
                        LEFT OUTER JOIN --Programming Cleansed Data
                        ( SELECT    bv.EXTENDEDDATA.EXT_INTEGER AS IsCleansed ,
                                    BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID]
                          FROM      BV.EXTENDEDDATA
                                    LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
                          WHERE     ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 622534631 )
                                    AND bv.EXTENDEDDATA.EXT_INTEGER != 1
                        ) S ON S.AssetID = R.AssetID
             ) B ON a.DCO_ASS_ID = b.AssetID
        JOIN bv.DEAL C ON c.DEA_ID = a.DCO_DEA_ID
        JOIN bv.MASTERDEAL D ON d.MDL_ID = c.DEA_MDL_ID
        JOIN pbs.DealProdAcQ E ON e.dea_id = c.DEA_ID
            join [RightsLineETL-Light].[dbo].[Apr23_Deals] Apr23 on Apr23.dea_id = e.dea_id
        LEFT JOIN trafficdata.BV.PBSDEALSTATUS pbsds ON pbsds.PBSDS_ID = c.DEA_PBSDS_ID
        LEFT JOIN [PBS].[MasterLookupValues] mlvPBSD_Label ON mlvPBSD_Label.bv_value = pbsds.PBSDS_LABEL
                                                              AND mlvPBSD_Label.ml_id = 2
                                                              AND field = 'trafficdata.BV.PBSDEALSTATUS.PBSDS_LABEL'
        LEFT JOIN [PBS].[MasterLookupValues] mlvWorkflowStatus ON mlvWorkflowStatus.ml_id = 2
                                                              AND mlvWorkflowStatus.field = 'static.WorkflowStatus.value'


