                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select	ParentID,
		ChildID
from
(
SELECT	DISTINCT 
		'Offer'+CAST(a.Dea_ID AS NVARCHAR(100)) as ParentID
		 ,'org'+CAST(a.Item_BV_ID AS NVARCHAR) + ':' +  'Offer' + cast(a.Dea_ID as nvarchar) + ':DealFunderID' +  cast(a.dealfundingid as nvarchar) as ChildID
FROM [TrafficData].[PBS].[vwFunders] a
join [RightsLineETL-Light].dbo.Apr23_Deals z on z.dea_id = a.Dea_ID
) q
where q.ChildID is not null


