        PBS.MoveArticleOfSpeach(CASE WHEN ass.ASS_EPISODETITLE IS NULL
                                     THEN 'Episode '
                                          + CAST([TrafficData].[PBS].[vwMOSEpisode].[Episode.Distribution.Number] AS VARCHAR(10))
                                     WHEN RTRIM(LTRIM(ass.ass_episodetitle)) = ''
