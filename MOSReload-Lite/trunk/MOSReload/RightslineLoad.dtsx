WITH    cteMergedDeals ( Dea_ID )
          AS ( SELECT	DISTINCT
                        c.DEA_ID
               FROM     TrafficData.BV.DEALCONTENT A
                        JOIN ( SELECT   R.AssetID ,
                                        Q.FranchiseName ,
                                        R.SeasonNumber ,
                                        S.IsCleansed
                               FROM     --Franchise Info
                                        ( SELECT    TrafficData.BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID] ,
                                                    TrafficData.BV.USERLOOKUP.ULU_DESC AS FranchiseName
                                          FROM      TrafficData.BV.EXTENDEDDATA
                                                    LEFT OUTER JOIN TrafficData.BV.USERLOOKUP ON TrafficData.BV.EXTENDEDDATA.EXT_INTEGER = TrafficData.BV.USERLOOKUP.ULU_ID
                                          WHERE     ( TrafficData.BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871743 )
                                        ) Q
                                        JOIN --Original Season Info
                                        ( SELECT    TrafficData.BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID] ,
                                                    TrafficData.BV.EXTENDEDDATA.EXT_INTEGER AS SeasonNumber
                                          FROM      TrafficData.BV.EXTENDEDDATA
                                                    LEFT OUTER JOIN TrafficData.BV.USERLOOKUP ON TrafficData.BV.EXTENDEDDATA.EXT_INTEGER = TrafficData.BV.USERLOOKUP.ULU_ID
                                          WHERE     ( TrafficData.BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871751 )
                                                    AND TrafficData.BV.EXTENDEDDATA.EXT_INTEGER &gt; 0
                                        ) R ON r.AssetID = q.AssetID
                                        LEFT OUTER JOIN --Programming Cleansed Data
                                        ( SELECT    TrafficData.BV.EXTENDEDDATA.EXT_INTEGER AS IsCleansed ,
                                                    TrafficData.BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID]
                                          FROM      TrafficData.BV.EXTENDEDDATA
                                                    LEFT OUTER JOIN TrafficData.BV.USERLOOKUP ON TrafficData.BV.EXTENDEDDATA.EXT_INTEGER = TrafficData.BV.USERLOOKUP.ULU_ID
                                          WHERE     ( TrafficData.BV.EXTENDEDDATA.EXT_SEXTC_ID = 622534631 )
                                                    AND TrafficData.BV.EXTENDEDDATA.EXT_INTEGER != 1
                                        ) S ON S.AssetID = R.AssetID
                             ) B ON a.DCO_ASS_ID = b.AssetID
                        JOIN TrafficData.BV.DEAL C ON c.DEA_ID = a.DCO_DEA_ID
                        JOIN TrafficData.BV.MASTERDEAL D ON d.MDL_ID = c.DEA_MDL_ID
             )
    SELECT DISTINCT
            paa.dea_id ,
            [Title] ,
            [AlliantID] ,
            CAST(CONVERT(VARCHAR, [Contract_Effective_Date], 101) AS NVARCHAR) AS [Contract_Effective_Date] ,
            CAST(CONVERT(VARCHAR, [Contract_Execution_Date], 101) AS NVARCHAR) AS [Contract_Execution_Date] ,
            [Programming_service] ,
            [RightsAndRestrictionsNotes] ,
            [Executed_Status] ,
            [financial_notes] ,
            [PBSCanSecureUnderWriting] AS [PBSCanSecureUnderWriting] ,
            [dealid] ,
            mlvPBSD_Label.rl_value AS entity_status ,
            mlvStaticSTCon.rl_value AS standard_terms__conditions ,
            CAST(mlvWorkflowStatus.rl_value AS INT) AS WorkflowStatus
    FROM    [TrafficData].[PBS].[DealPBSPAA1] paa 
            join [RightsLineETL-Light].[dbo].Apr23_deals Apr23 on Apr23.dea_id = paa.dea_id
            LEFT JOIN [TrafficData].[BV].[DEAL] ds ON ds.DEA_ID = paa.dea_id
            LEFT JOIN trafficdata.BV.PBSDEALSTATUS pbsds ON pbsds.PBSDS_ID = ds.DEA_PBSDS_ID
            LEFT JOIN [PBS].[MasterLookupValues] mlvPBSD_Label ON mlvPBSD_Label.bv_value = pbsds.PBSDS_LABEL
                                                              AND mlvPBSD_Label.ml_id = 1
                                                              AND field = 'trafficdata.BV.PBSDEALSTATUS.PBSDS_LABEL'
            LEFT JOIN [PBS].[MasterLookupValues] mlvStaticSTCon ON mlvStaticSTCon.ml_id = 1
                                                              AND mlvStaticSTCon.field = 'static.standard_terms__conditions.value'
            LEFT JOIN [PBS].[MasterLookupValues] mlvWorkflowStatus ON mlvWorkflowStatus.ml_id = 1
                                                              AND mlvWorkflowStatus.field = 'static.WorkflowStatus.value'
    WHERE   NOT EXISTS ( SELECT zzz.DEA_ID
                         FROM   cteMergedDeals zzz
                         WHERE  zzz.DEA_ID = paa.dea_id )

