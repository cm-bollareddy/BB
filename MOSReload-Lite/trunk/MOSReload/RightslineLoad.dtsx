                  contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
                  description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
                  name="Source BV-Deal MasterDeal and DealContent"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select	distinct 'MasterOffer' + cast(d.MDL_ID as nvarchar(100)) as Grandparent,
		'Offer' + cast(c.DEA_ID as nvarchar(100)) as Parent,
		a.DCO_ASS_ID as child
from	TrafficData.BV.DEALCONTENT A
join
(
SELECT  R.AssetID,Q.FranchiseName, R.SeasonNumber, S.IsCleansed
FROM  
	--Franchise Info
	( SELECT    TrafficData.BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID],TrafficData.BV.USERLOOKUP.ULU_DESC As FranchiseName
        FROM      TrafficData.BV.EXTENDEDDATA
                LEFT OUTER JOIN TrafficData.BV.USERLOOKUP ON TrafficData.BV.EXTENDEDDATA.EXT_INTEGER = TrafficData.BV.USERLOOKUP.ULU_ID
        WHERE     ( TrafficData.BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871743 )
    ) Q
    JOIN 
	--Original Season Info
	( SELECT   TrafficData.BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID], TrafficData.BV.EXTENDEDDATA.EXT_INTEGER as SeasonNumber
            FROM     TrafficData.BV.EXTENDEDDATA
                    LEFT OUTER JOIN TrafficData.BV.USERLOOKUP ON TrafficData.BV.EXTENDEDDATA.EXT_INTEGER = TrafficData.BV.USERLOOKUP.ULU_ID
            WHERE    ( TrafficData.BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871751 )
			and		TrafficData.BV.EXTENDEDDATA.EXT_INTEGER &gt; 0
    ) R ON r.AssetID = q.AssetID
	
    left outer JOIN 
	--Programming Cleansed Data
	( SELECT   TrafficData.BV.EXTENDEDDATA.EXT_INTEGER as IsCleansed, TrafficData.BV.EXTENDEDDATA.EXT_OWNERRECORD_ID AS [AssetID]
            FROM     TrafficData.BV.EXTENDEDDATA
                    LEFT OUTER JOIN TrafficData.BV.USERLOOKUP ON TrafficData.BV.EXTENDEDDATA.EXT_INTEGER = TrafficData.BV.USERLOOKUP.ULU_ID
            WHERE    ( TrafficData.BV.EXTENDEDDATA.EXT_SEXTC_ID = 622534631 )
			and		TrafficData.BV.EXTENDEDDATA.EXT_INTEGER != 1
    ) S ON S.AssetID = R.AssetID
	
) B on a.DCO_ASS_ID = b.AssetID
Join [RightsLineETL-Light].[dbo].Apr23_Episodes feb28 on  feb28.ASS_ID = b.AssetID
join TrafficData.BV.DEAL C on c.DEA_ID = a.DCO_DEA_ID
join TrafficData.BV.MASTERDEAL D on d.MDL_ID = c.DEA_MDL_ID</property>
