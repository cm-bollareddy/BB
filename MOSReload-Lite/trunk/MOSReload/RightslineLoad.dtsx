        LEFT JOIN bv.SYSLOOKUP ARLookup ON ARLookup.SLU_ID = dea.DEA_SLU_ID_ASPECTRATIO
        LEFT JOIN [PBS].[MasterLookupValues] mlvAspectRation ON mlvAspectRation.bv_value = ARLookup.SLU_DESC
