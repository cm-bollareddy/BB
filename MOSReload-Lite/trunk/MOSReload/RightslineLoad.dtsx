                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
        z.ParentID ,
        z.ChildID
FROM    (
		  --Parent Season
          SELECT	DISTINCT
                    b.rights_id AS ParentID ,
                    c.ASS_ID AS ChildID
          FROM      pbs.Alliant_NolaRights a
                    JOIN [PBS].[Alliant_Rights] b ON b.rights_id = a.Rights_id
                    JOIN ( SELECT DISTINCT
                                    ASS_EPISODESLATE ,
                                    a.ass_id
                           FROM     bv.ASSET a
                                    JOIN ( SELECT   vwMosEpisode.[ass_id]
                                           FROM     [TrafficData].[PBS].[vwMOSEpisode]
                                                    JOIN Bv.EXTENDEDDATA extD ON extD.EXT_OWNERRECORD_ID = ass_id
                                                    JOIN bv.USERLOOKUP ulkup ON ulkup.Ulu_id = extD.EXT_INTEGER
                                                    JOIN [RightsLineETL-Light].[dbo].Apr23_Episodes feb28 ON feb28.ASS_ID = vwMOSEpisode.Ass_id
                                                    JOIN bv.asset ass ON ass.ass_id = vwMOSEpisode.ass_id
                                                    LEFT JOIN [BV].[ASSETCATEGORY] aca ON aca.ACA_ID = ass.ASS_ACA_ID
                                                    LEFT JOIN bv.vchip vchip ON vchip.VCH_ID = ass.ASS_VCH_ID
                                                    LEFT JOIN bv.Deal dea ON dea.DEA_ID = [TrafficData].[PBS].[vwMOSEpisode].DealID
                                                    LEFT JOIN bv.SYSLOOKUP ARLookup ON ARLookup.SLU_ID = dea.DEA_SLU_ID_ASPECTRATIO
                                                    LEFT JOIN ( SELECT
                                                              BV.EXTENDEDDATA.EXT_INTEGER ,
                                                              BV.USERLOOKUP.ULU_DESC ,
                                                              BV.EXTENDEDDATA.EXT_OWNERRECORD_ID
                                                              FROM
                                                              BV.EXTENDEDDATA
                                                              LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
                                                              WHERE
                                                              ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871743 )
                                                              ) P ON P.EXT_OWNERRECORD_ID = ass.ASS_ID
                                                    LEFT JOIN bv.language subLan ON sublan.lan_id = dea.DEA_LAN_ID_SUBTITLES
                                           WHERE    EXT_SEXTC_ID = 607871753 -- RightslineTemplate Column
                                                    AND ulkup.ULU_CLASS = 'L03'
                                                    AND ULU_ID != 609587740
                                                    AND CHARINDEX('Season',
                                                              [Episode.ParentID]) &gt;= 1
                                         ) B ON a.ass_id = b.ass_id
                         ) C ON c.ASS_EPISODESLATE = a.NolaCode
