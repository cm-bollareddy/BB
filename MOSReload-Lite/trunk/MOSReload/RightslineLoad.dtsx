                        vwMOsEpisode.[Episode.ParentID]
               FROM     pbs.vwMOSEpisode
                        JOIN MiEpisodeCTE ON MIEpisodeCTE.episodeNO = vwMOSEpisode.[ass_id]
                                             AND MIEpisodeCTE.[Episode.parentID] = vwMOsEpisode.[Episode.ParentID]
               GROUP BY vwMOSEpisode.[Episode.ParentID]
             )
    SELECT  [InternalPBSID] ,
            [Season.Season Class] ,
            [Season.Referent Type] ,
            [Season.UID] ,
            [Season.ContentIdentifier] ,
            [Season.Sequence.Number] ,
            CAST(minReleaseDate AS NVARCHAR(30)) AS [Season.ReleaseDate] ,
            [Season.ParentID] ,
            [Season.Title] ,
            SUBSTRING([season.ParentID],
                      CHARINDEX(':Series', [Season.ParentID]) + 7,
                      LEN([Season.ParentID])) AS PBS_Series_ID ,
            SUBSTRING([season.UID], CHARINDEX(':Season', [Season.UID]) + 1,
                      LEN([Season.UID])) AS PBS_Season_ID ,
            ass.ASS_PBSNOLAROOT_CM AS pbsnolaroot ,
            'Executive' AS [Season.ExecutiveProducer.OrganizationRole] ,
            ExeCom.com_name AS [Season.ExecutiveProducer.OrganizationName] ,
            'Producer' AS [Season.Producer.OrganizationRole] ,
            ProdCom.com_name AS [Season.Producer.OrganizationName] ,
            mlvLanguage.rl_value AS Language ,
            mlvTerritory.rl_value AS Territory ,
            mlvTemplate.rl_value AS Template ,
            CAST(mlvEntityStatus.RL_Value AS NVARCHAR(20)) AS entity_status
    FROM    [PBS].[vwMOSSeason]
            JOIN bv.asset ass ON ass.ass_id = vwMOSSeason.InternalPBSID
            JOIN [RightsLineETL-Light].[dbo].[Apr23_Season] Apr23 ON Apr23.SeasonUID = SUBSTRING([season.UID],
                                                              CHARINDEX(':Season',
                                                              [Season.UID])
                                                              + 1,
                                                              LEN([Season.UID]))
            LEFT JOIN MIReleaseCTE ON MIReleaseCTE.[Episode.ParentID] = [Season.UID]
            JOIN [RightsLineETL-Light].[dbo].[xref_DealToSeason] xref ON xref.childid = REPLACE([Season.UID],
                                                              'cid:org:pbs.org:',
                                                              '')
            LEFT JOIN bv.PBSDEALCONTACT ExecProd ON Execprod.pbsdc_dea_id = REPLACE(xref.ParentID,
                                                              'Offer',
                                                              '')
                                                    AND ( ExecProd.PBSDC_SLU_ID_CONTACTTYPE = 44001 )
            LEFT JOIN bv.PBSDEALCONTACT Producer ON Producer.pbsdc_dea_id = REPLACE(xref.ParentID,
                                                              'Offer',
                                                              '')
                                                    AND ( Producer.PBSDC_SLU_ID_CONTACTTYPE = 44002 )
            LEFT JOIN bv.company ExeCom ON ExecProd.PBSDC_COM_ID = ExeCom.COM_ID
            LEFT JOIN bv.company ProdCom ON Producer.PBSDC_COM_ID = ProdCom.COM_ID
            LEFT JOIN [PBS].[MasterLookupValues] mlvLanguage ON mlvLanguage.field = 'static.Language.value'
                                                              AND mlvLanguage.ML_ID = 10
            LEFT JOIN [PBS].[MasterLookupValues] mlvTerritory ON mlvTerritory.field = 'static.Territory.value'
                                                              AND mlvTerritory.ML_ID = 10
            LEFT JOIN [PBS].[MasterLookupValues] mlvTemplate ON mlvTemplate.field = 'static.Template.value'
                                                              AND mlvTemplate.ML_ID = 10
            LEFT JOIN [PBS].[MasterLookupValues] mlvEntityStatus ON mlvEntityStatus.field = 'static.entity_status.value'
                                                              AND mlvEntityStatus.ML_ID = 10
