	INNER JOIN uv_contract_udf AS cu ON c.contract_sid = cu.contract_sid
	left outer join x_contract x on x.contract_sid = c.contract_sid
	left JOIN    x_note n ON x.note_sid = n.note_sid
	left JOIN    x_note_attachment a ON n.note_body_seq_nbr = a.note_body_seq_nbr
WHERE     (c.contract_status_display_value &lt;&gt; 'Prior revision') AND (c.contract_id LIKE '%%')
