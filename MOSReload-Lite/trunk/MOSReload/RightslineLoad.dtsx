                                                              BV.EXTENDEDDATA.EXT_OWNERRECORD_ID
                                                              FROM
                                                              BV.EXTENDEDDATA
                                                              LEFT OUTER JOIN BV.USERLOOKUP ON BV.EXTENDEDDATA.EXT_INTEGER = BV.USERLOOKUP.ULU_ID
                                                              WHERE
                                                              ( BV.EXTENDEDDATA.EXT_SEXTC_ID = 607871743 )
                                                              ) P ON P.EXT_OWNERRECORD_ID = ass.ASS_ID
                                                    LEFT JOIN bv.language subLan ON sublan.lan_id = dea.DEA_LAN_ID_SUBTITLES
                                           WHERE    EXT_SEXTC_ID = 607871753
                                                    AND ulkup.ULU_CLASS = 'L03'
                                                    AND ULU_ID = 609587740
                                         ) Q ON q.ass_id = a.ASS_ID
