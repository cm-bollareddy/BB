                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  ip.ip_rights_sid AS Rights_ID ,
        ip.rights_type_sid ,
        'Rights_Type' = CASE WHEN ip.rights_type_sid = 1 THEN 'OWNED'
                             WHEN ip.rights_type_sid = 2 THEN 'LICENSE'
                             WHEN ip.rights_type_sid = 3 THEN 'OPTION'
                             WHEN ip.rights_type_sid = 4
                             THEN 'BLACKOUT_LICENSEE'
                             WHEN ip.rights_type_sid = 5
                             THEN 'BLACKOUT_LICENSOR'
                             WHEN ip.rights_type_sid = 6
                             THEN 'RESTRICTION_LICENSEE'
                             WHEN ip.rights_type_sid = 7
                             THEN 'RESTRICTION_LICENSOR'
                             WHEN ip.rights_type_sid = 8 THEN 'EXCLUSION'
                             WHEN ip.rights_type_sid = 9
                             THEN 'Licensee Holdback'
                             ELSE 'HOLDBACK_LICENSOR'
                        END ,
        asset.udkey10_Description AssetTitle ,
        assetlist.descr AS AssetList ,
        LTRIM(RTRIM(SUBSTRING(asset.udkey10_description,
                              LEN(asset.udkey10_description) - 12,
                              LEN(asset.udkey10_description)))) AS NOLACode
FROM    uv_udkey_10 asset ( NOLOCK )
        JOIN c_udkey_10_group_dtl list ( NOLOCK ) ON list.udkey_10_sid = asset.udkey_10_sid
        JOIN c_udkey_10_group_hdr assetlist ( NOLOCK ) ON list.udkey_10_group_sid = assetlist.udkey_10_group_sid
        JOIN dbo.x_ip_rights ip ( NOLOCK ) ON asset.udkey_10_sid = ip.udkey_10_sid
                                              OR ip.udkey_10_group_sid = assetlist.udkey_10_group_sid
WHERE   ip.rights_type_sid IN ( 2, 3, 9 )</property>
