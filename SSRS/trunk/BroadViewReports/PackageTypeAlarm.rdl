  <DataSets>
    <DataSet Name="AlarmPackage">
      <Query>
        <DataSourceName>BroadViewDataSource</DataSourceName>
        <CommandText>SELECT DISTINCT (vet.vet_desc)
FROM         versiontype vet LEFT JOIN
                      pbstranscodetype_versiontype pttvt ON pttvt.ttvt_vet_id = vet.vet_id
WHERE     vet.vet_isarchived = 0 AND vet.vet_desc LIKE 'HD%' AND
                          ((SELECT     COUNT(*)
                              FROM         pbstranscodetype_versiontype pttvt
                              WHERE     pttvt.ttvt_vet_id = vet.vet_id) = 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="VET_DESC">
          <DataField>VET_DESC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PBSRaiseError">
      <Query>
        <DataSourceName>BizTalkDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pi_int">
            <Value>=Parameters!pi_int.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandType>StoredProcedure</CommandType>
        <CommandText>PBSRaiseError</CommandText>
      </Query>
      <Fields>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AlarmPackageCount">
      <Query>
        <DataSourceName>BroadViewDataSource</DataSourceName>
        <CommandText>SELECT     COUNT(DISTINCT vet.vet_desc) AS MyCount
FROM         versiontype AS vet LEFT OUTER JOIN
                      pbstranscodetype_versiontype AS pttvt ON pttvt.ttvt_vet_id = vet.vet_id
WHERE     (vet.vet_isarchived = 0) AND (vet.vet_desc LIKE 'HD%') AND
                          ((SELECT     COUNT(*) AS Expr1
                              FROM         pbstranscodetype_versiontype AS pttvt
                              WHERE     (pttvt.ttvt_vet_id = vet.vet_id)) = 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="MYCOUNT">
          <DataField>MYCOUNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ServerDetail">
      <Query>
        <DataSourceName>SSRSDataSource</DataSourceName>
        <CommandText>SELECT DISTINCT C2.Name AS Data_Source_Name, C.Name AS Dependent_Item_Name, C.Path AS Dependent_Item_Path
FROM         DataSource AS DS INNER JOIN
                      Catalog AS C ON DS.ItemID = C.ItemID AND DS.Link IN
                          (SELECT     ItemID
                            FROM          Catalog
                            WHERE      (Type = 5)) FULL OUTER JOIN
                      Catalog AS C2 ON DS.Link = C2.ItemID
WHERE     (C2.Type = 5) AND (C.Name = 'PackageTypeAlarm') AND (C.Path = '/BroadView/PackageTypeAlarm') AND (C2.Name LIKE 'BroadView%')
ORDER BY Data_Source_Name, Dependent_Item_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Data_Source_Name">
          <DataField>Data_Source_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Dependent_Item_Name">
          <DataField>Dependent_Item_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Dependent_Item_Path">
          <DataField>Dependent_Item_Path</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
