  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>BroadViewDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!End_Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with CTE as
(
SELECT
  dat_date,
  (select cha_desc from channel where cha_id = 5000000) NR01,
  (select sum(tln_duration)
     from timeline
     join version on ver_id = tln_ver_id
     where
        tln_cha_id = 5000000
        and
        tln_logdate = DAT_DATE
        and
        tln_level = 1                --- no traffic items
        and -
        tln_ver_id is not null       --- package is assigned to schedule
        and -
        ver_islive = 0           -- package is not live
       and
       ver_ispbsdistributordirect = 0      -- not distributor package
       and
       ver_ispackage = 1
 ) NR01SUM,
  (select cha_desc from channel where cha_id = 7400) NR02,
  (select sum(tln_duration)
     from timeline
     join version on ver_id = tln_ver_id
     where
        tln_cha_id = 7400
        and
        tln_logdate = DAT_DATE
        and
--        tln_level = 1                --- no traffic items
--        and -
        tln_ver_id is not null       --- package is assigned to schedule
--        and
--        ver_islive = 0           -- package is not live
--        and
--        ver_ispbsdistributordirect = 0      -- not distributor package
--        and
--        ver_ispackage = 1
) NR02SUM
FROM
  dates
  where
 -- dat_date between  F_AddDay( 'TODAY', -120) and F_AddDay( 'TODAY', 90)
 dat_date between ? and ?

Order by 1
)
select
dat_date
,NR01SUM as NR01Seconds
,F_DURATIONTOSTR(NR01SUM) as NR01Display
,NR02SUM as NR02Seconds
,F_DURATIONTOSTR(NR02SUM) as NR02Display

,(NR01SUM + coalesce(NR02SUM,0)) as NRTTotalSeconds
,F_DURATIONTOSTR(NR01SUM + coalesce(NR02SUM,0)) as NRTTotalDisplay
from
CTE
order by dat_date</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DAT_DATE">
          <DataField>DAT_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="NR01SECONDS">
