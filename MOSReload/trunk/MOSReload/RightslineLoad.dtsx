		left join [PBS].[MasterLookupValues] mlvTerritory on mlvTerritory.field = 'static.Territory.value'
							and mlvTerritory.ML_ID = 7
		left join [PBS].[MasterLookupValues] mlvdestExclusive on mlvdestExclusive.field = 'PBS.vwMOSUsage.Usage.EffectiveTerm.Exclusive'
							and mlvdestExclusive.dbvalue = [Usage.EffectiveTerm.Exclusive]
							and mlvdestExclusive.ML_ID = 7
		left join [PBS].[MasterLookupValues] mlvHoldBackMedia on mlvHoldBackMedia.field = 'static.Holdback_Media.value'
							and mlvHoldBackMedia.ML_ID = 7
