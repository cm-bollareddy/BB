                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  vwMOsepisode.[ass_id] ,
        [Episode.ContentIdentifier] ,
        [Episode.UID] ,
        [Episode.ParentID] ,
        [Episode.Referent.Type] ,
        [Episode.Organization.Contact.Info.Phone] ,
        [Episode.Organization.Contact.Info.Phone.Type] ,
        [Episode.Organization.Contact Info.Address] ,
        [Episode.Organization.Contact.Info.PrimaryEmail] ,
        [Episode.Organization.Contact.Info.Name] ,
        [Episode.Organization.Contact.Info.ContactID] ,
        [Episode.Organization.DisplayName] ,
        [Episode.Organization.domain] ,
        [Episode.Organization.OrganizationID] ,
        [Episode.Organization.Role] ,
        PBS.MoveArticleOfSpeach(CASE WHEN ass.ASS_EPISODETITLE IS NOT NULL
                                     THEN RTRIM(LTRIM(ass.ASS_EPISODETITLE))
                                     ELSE 'Episode '
                                          + CAST([TrafficData].[PBS].[vwMOSEpisode].[Episode.EpisodeNumber] AS VARCHAR(10))
                                END) AS [Episode.Title.Corrected] ,
        PBS.MoveArticleOfSpeach(CASE WHEN ass.ASS_EPISODETITLE IS NOT NULL
                                     THEN SUBSTRING(RTRIM(LTRIM(ass.ass_episodetitle)),
                                                    1, 120)
                                     ELSE 'Episode '
                                          + CAST([TrafficData].[PBS].[vwMOSEpisode].[Episode.EpisodeNumber] AS VARCHAR(10))
                                END) AS [Episode.Title120.Corrected] ,
        [Episode.TitleClass] ,
        [Episode.EpisodeNumber] ,
        [Episode.EpisodeClass] ,
        CAST([Episode.SynopsisShort] AS NVARCHAR(400)) AS [Episode.SynopsisShort] ,
        CAST([Episode.SynopsisLong] AS NVARCHAR(400)) AS [Episode.SynopsisLong] ,
        [Episode.CountryOfOrigin] ,
        CAST([Episode.PrimaryGenre] AS NVARCHAR(500)) AS [Episode.PrimaryGenre] ,
        CAST(REPLACE([Episode.SecondaryGenre], ',', '|') AS NVARCHAR(500)) AS [Episode.SecondaryGenre] ,
        [Episode.Distribution.Number] ,
        [Episode.ReleaseDate] ,
        ass.ASS_DURATIONSCHEDULED AS [Episode.ApproximateLength] ,
        [Episode.Language.Primary.mode] ,
        [Episode.LanguagePrimary] ,
        [Episode.LanguageSecondary] ,
        [Episode.Credits.Display.Name] ,
        [Episode.Credits.Role] ,
        [Episode.Credits.TalentID] ,
        [Episode.ContactType] ,
        SUBSTRING([Episode.ParentID],
                  CHARINDEX(':pbs.org', [Episode.ParentID]) + 9,
                  LEN([Episode.ParentID])) AS PBS_Parent_ID ,
        SUBSTRING([Episode.ContentIdentifier],
                  CHARINDEX('NOLARoot', [Episode.ContentIdentifier]) + 9, 4) AS NolaRoot ,
        SUBSTRING([Episode.ContentIdentifier],
                  CHARINDEX('NOLARoot', [Episode.ContentIdentifier]) + 9,
                  LEN([Episode.ContentIdentifier])) AS Nolacode ,
        mlvAssIsPublished.DerivedValue AS entity_status ,
        mlvWorkflowStatus.DerivedValue AS WorkflowStatus ,
        CASE [Episode.TitleClass]
          WHEN 'Working'
          THEN SUBSTRING(RTRIM(LTRIM(ass.ass_episodetitle)), 1, 120)
          ELSE ''
        END AS additional_akas ,
        lookRating.RAT_DESC AS us_tvpg ,
        YEAR([Episode.ReleaseDate]) AS first_broadcast_year ,
        ARLookup.SLU_DESC AS aspect_ratio ,
        CASE dea.DEA_SLU_ID_HIDEF
          WHEN 1 THEN CAST('HD' AS NVARCHAR)
          ELSE CAST('SD' AS NVARCHAR)
        END AS resolution ,
        mlvISPBSMINORITYPRODUCTION.DerivedValue AS minority_production ,
        mlvEditClass.DerivedValue AS edit_class ,
        mlvEditName.DerivedValue AS EditName ,
        mlvEpisodeEntityTemplate.DerivedValue AS EpisodeEntityTemplate
FROM    [TrafficData].[PBS].[vwMOSEpisode]
        JOIN Bv.EXTENDEDDATA extD ON extD.EXT_OWNERRECORD_ID = ass_id
        JOIN bv.USERLOOKUP ulkup ON ulkup.Ulu_id = extD.EXT_INTEGER
        JOIN [RightsLineETL].[dbo].Apr23_OneTimeOnly Apr23 ON Apr23.ASS_ID = vwMOSEpisode.Ass_id
        JOIN Bv.Asset ass ON ass.ass_id = vwMOSEpisode.Ass_id
        LEFT JOIN bv.Rating lookRating ON lookRating.RAT_ID = ass.ASS_RAT_ID
        LEFT JOIN bv.Deal dea ON dea.DEA_ID = [TrafficData].[PBS].[vwMOSEpisode].DealID
        LEFT JOIN bv.SYSLOOKUP ARLookup ON ARLookup.SLU_ID = dea.DEA_SLU_ID_ASPECTRATIO
        LEFT JOIN [PBS].[MasterLookupValues] mlvISPBSMINORITYPRODUCTION ON CAST(mlvISPBSMINORITYPRODUCTION.dbvalue AS INT) = dea.DEA_ISPBSMINORITYPRODUCTION
                                                              AND mlvISPBSMINORITYPRODUCTION.field = 'trafficdata.BV.DEAL.DEA_ISPBSMINORITYPRODUCTION'
                                                              AND mlvISPBSMINORITYPRODUCTION.ML_ID = 5
        LEFT JOIN [PBS].[MasterLookupValues] mlvEditClass ON mlvEditClass.field = 'static.edit_class.value'
                                                             AND mlvEditClass.ML_ID = 5
        LEFT JOIN [PBS].[MasterLookupValues] mlvAssIsPublished ON CAST(mlvAssIsPublished.dbvalue AS INT) = ass.ASS_ISPBSPUBLISHED
                                                              AND mlvAssIsPublished.field = 'trafficdata.BV.ASSET.ASS_ISPBSPUBLISHED'
                                                              AND mlvAssIsPublished.ML_ID = 5
        LEFT JOIN [PBS].[MasterLookupValues] mlvWorkflowStatus ON mlvWorkflowStatus.field = 'static.WorkflowStatus.value'
                                                              AND mlvWorkflowStatus.ML_ID = 5
        LEFT JOIN [PBS].[MasterLookupValues] mlvEditName ON mlvEditName.field = 'static.EditName.value'
                                                            AND mlvEditName.ML_ID = 5
        LEFT JOIN [PBS].[MasterLookupValues] mlvEpisodeEntityTemplate ON mlvEpisodeEntityTemplate.field = 'static.EpisodeEntityTemplate.value'
                                                              AND mlvEpisodeEntityTemplate.ML_ID = 5
WHERE   EXT_SEXTC_ID = 607871753
        AND ulkup.ULU_CLASS = 'L03'
        AND ULU_ID = 609587740

