        [Usage.EffectiveTerm.Class] ,
        [Usage.EffectiveTerm.Start] ,
        [Usage.EffectiveTerm.End] ,
        [Usage.EffectiveTerm.Start.OriginalBroadcastBased] ,
        [Usage.EffectiveTerm.Exclusive] ,
        [Usage.EffectiveTerm.Play.Model] ,
        [Usage.EffectiveTerm.Play.RecurrenceDayCount] ,
        mlvHoldBackMedia.DerivedValue AS HoldbackMedia ,
        mlvdestExclusive.DerivedValue AS 'Exclusivity' ,
        SUBSTRING(vwMOSUsage.[Usage.UID], LEN('usageid:org:pbs.org:') + 1,
                  LEN(vwMOSUsage.[Usage.UID])) AS [item_bv_id] ,
        SUBSTRING(vwMOSUsage.[Usage.ParentID],
                  LEN('parentid:org:pbs.org:') + 1,
                  LEN(vwMOSUsage.[Usage.ParentID])) AS [Item_BV_Parent_ID],
				  mlvTitle.DerivedValue as Title,
				  mlvTerritory.DerivedValue as Territory
FROM    [PBS].[vwMOSUsage]
        JOIN [rightslineETL].[dbo].Apr23_deals Apr23 ON Apr23.OfferUID = SUBSTRING(vwMOSUsage.[Usage.ParentID],
