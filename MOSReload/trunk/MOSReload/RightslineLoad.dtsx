      ,[Episode.LanguageSecondary]
      ,[Episode.Credits.Display.Name]
      ,[Episode.Credits.Role]
      ,[Episode.Credits.TalentID]
      ,[Episode.ContactType]
	  ,substring([Episode.ParentID],charindex(':pbs.org',[Episode.ParentID])+9,len([Episode.ParentID])) as PBS_Parent_ID
	  ,substring([Episode.ContentIdentifier],charindex('NOLARoot',[Episode.ContentIdentifier])+9,4) as NolaRoot
	  ,substring([Episode.ContentIdentifier],charindex('NOLARoot',[Episode.ContentIdentifier])+9,len([Episode.ParentID])) as Nolacode
	  ,mlvEpisodeEditName.DerivedValue as EditName
	  ,mlvEpisodeEntityTemplate.DerivedValue as EpisodeEntityTemplate
  FROM [TrafficData].[PBS].[vwMOSEpisode] 
  Join Bv.EXTENDEDDATA extD on extD.EXT_OWNERRECORD_ID = ass_id 
  Join bv.USERLOOKUP ulkup on ulkup.Ulu_id = extD.EXT_INTEGER
  Join [RightsLineETL].[dbo].Apr23_Episodes Apr23 on  Apr23.ASS_ID = vwMOSEpisode.Ass_id
  left join [PBS].[MasterLookupValues] mlvEpisodeEditName on mlvEpisodeEditName.field = 'static.EditName.value'
						and mlvEpisodeEditName.ML_ID = 4
  left join [PBS].[MasterLookupValues] mlvEpisodeEntityTemplate on mlvEpisodeEntityTemplate.field = 'static.EpisodeEntityTemplate.value'
			and mlvEpisodeEntityTemplate.ML_ID = 4
  where EXT_SEXTC_ID = 607871753 
  and ulkup.ULU_CLASS = 'L03' and ULU_ID != 609587740
  and charindex('Season',[Episode.ParentID]) = 0</property>
