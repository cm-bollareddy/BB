/*
select	 distinct *
from
(
	Select
      'Funding Information (Combined)'as entity_title,
      '47' as entity_template,
	  BV.COMPANY.COM_NAME as underwriter,
      case BV.deal.DEA_SLU_ID_PBSUNDERWRITINGTYPE
			when 48001 then 'Available'
			when 48002 then 'Not Available'
			when 48003 then 'Available except to Competitors (List Below)'
			when 48004 then 'Available with Producer Approval (Describe Below)'
			else 'Not Available'
		end as local_underwriting_option,
      '' as explanation_and_notes,
      case 
	  	when BV.COMPANY.COM_NAME LIKE 'PBS%' then 'Internal'
		else 'External'
	  end as accounting_class,
	  case BV.DEALFUNDING.DEF_SLU_ID_FUNDINGTYPE
			when 32002 then 'Production'
			when 32003 then 'Production'
			when 32001 then 'Acquisition'
			else ''
	  end as [funding_type],
      BV.DEALFUNDING.DEF_ISCREDITED as on_air_credit,

      --'Other' as [funder_role],

	case
		when BV.DEALFUNDING.DEF_ISPRODUCERFUNDING = 1 then 'Producer'
	
		when BV.DEALFUNDING.DEF_ISPRODUCERFUNDING = 0 then
			case
				when BV.COMPANY.COM_NAME LIKE 'PBS%' then 'Distributor'
				else 'Underwriter'
			end
		else 'Other'
	end as funder_role,

      BV.DEALFUNDING.DEF_AMOUNT as table_fee,
      BV.DEALFUNDING.DEF_BEGINDATE as [start],
      BV.DEALFUNDING.DEF_ENDDATE as [end],
      BV.DEALFUNDING.DEF_FISCALYEAR as [fiscal_year],
      '' as notes ,
      BV.DEALFUNDING.DEF_ID as pbs_traffic_id ,
	  BV.DEALFUNDING.DEF_DEA_ID as [Dea_ID],
	  BV.DEALFUNDING.DEF_ID as [Item_BV_ID] 
FROM    BV.DEALFUNDING
        JOIN BV.COMPANY ON BV.DEALFUNDING.DEF_COM_ID_FUNDER = BV.COMPANY.COM_ID
		join BV.deal on bv.deal.DEA_ID = BV.DEALFUNDING.DEF_DEA_ID
WHERE   ( BV.DEALFUNDING.DEF_ISPRODUCERFUNDING = 0 )
        AND ( BV.COMPANY.COM_NAME LIKE N'PBS%' )
        AND ( BV.COMPANY.COM_ISPRODUCER = 0 )	
union
	Select
      'Funding Information (Combined)'as entity_title,
      '47' as entity_template,
	  BV.COMPANY.COM_NAME as underwriter,
      case BV.deal.DEA_SLU_ID_PBSUNDERWRITINGTYPE
			when 48001 then 'Available'
			when 48002 then 'Not Available'
			when 48003 then 'Available except to Competitors (List Below)'
			when 48004 then 'Available with Producer Approval (Describe Below)'
			else 'Not Available'
		end as local_underwriting_option,
      '' as explanation_and_notes,
      case 
	  	when BV.COMPANY.COM_NAME LIKE 'PBS%' then 'Internal'
		else 'External'
	  end as accounting_class,
	  case BV.MASTERDEALFUNDING.MDF_SLU_ID_FUNDINGTYPE
			when 32002 then 'Production'
			when 32003 then 'Production'
			when 32001 then 'Acquisition'
			else ''
	  end as [funding_type],
      BV.MASTERDEALFUNDING.MDF_ISCREDITED as on_air_credit,

      --'Other' as [funding_role],

	case
		when BV.MASTERDEALFUNDING.MDF_ISPRODUCERFUNDING = 1 then 'Producer'
	
		when BV.MASTERDEALFUNDING.MDF_ISPRODUCERFUNDING = 0 then
			case
				when BV.COMPANY.COM_NAME LIKE 'PBS%' then 'Distributor'
				else 'Underwriter'
			end
		else 'Other'
	end as funder_role,


      BV.MASTERDEALFUNDING.MDF_AMOUNT as table_fee,
      BV.MASTERDEALFUNDING.MDF_BEGINDATE as [start],
      BV.MASTERDEALFUNDING.MDF_ENDDATE as [end],
      BV.MASTERDEALFUNDING.MDF_FISCALYEAR as [fiscal_year],
      '' as notes ,
      BV.MASTERDEALFUNDING.MDF_ID as pbs_traffic_id ,
	  bv.deal.DEA_ID as [Dea_ID],
	  BV.MASTERDEALFUNDING.MDF_ID as [Item_BV_ID] 
FROM    BV.MASTERDEALFUNDING
		join bv.deal on BV.MASTERDEALFUNDING.MDF_MDL_ID = bv.deal.DEA_MDL_ID
        JOIN BV.COMPANY ON BV.MASTERDEALFUNDING.MDF_COM_ID_FUNDER = BV.COMPANY.COM_ID
        JOIN bv.SYSLOOKUP z ON z.SLU_ID = BV.MASTERDEALFUNDING.MDF_SLU_ID_FUNDINGTYPE
WHERE   ( BV.MASTERDEALFUNDING.MDF_ISPRODUCERFUNDING = 0 )
) as q

*/

