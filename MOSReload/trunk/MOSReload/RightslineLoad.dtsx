		end 	as [DisplayFriendlyTitle]
	  ,case ass.ASS_ISPBSPUBLISHED
		when 1 then cast('Ready for External Publishing' as nvarchar)
		else cast('In Progress' as nvarchar)
	end as entity_status
	,11 as WorkflowStatus
	,case [Episode.TitleClass] 
		when 'Working' then 
			cast(substring(rtrim(ltrim(ass.ass_episodetitle)),1,120) as nvarchar(120))
		else cast('' as nvarchar)

	end as additional_akas
	,lookRating.RAT_DESC as us_tvpg
   ,year([Episode.ReleaseDate]) as first_broadcast_year
   ,ARLookup.SLU_DESC as aspect_ratio
,case dea.DEA_SLU_ID_HIDEF
          when 1 then cast( 'HD' as nvarchar(100))
          else cast( 'SD' as nvarchar(100))
  end as resolution
  FROM [TrafficData].[PBS].[vwMOSEpisode] 
  Join Bv.EXTENDEDDATA extD on extD.EXT_OWNERRECORD_ID = ass_id 
  Join bv.USERLOOKUP ulkup on ulkup.Ulu_id = extD.EXT_INTEGER
  Join [RightsLineETL].[dbo].Apr23_Episodes feb28 on  feb28.ASS_ID = vwMOSEpisode.Ass_id
  join bv.asset ass on ass.ass_id = vwMOSEpisode.ass_id
  LEFT JOIN [BV].[ASSETCATEGORY] aca ON aca.ACA_ID = ass.ASS_ACA_ID
  left join	bv.Rating lookRating on lookRating.RAT_ID = ass.ASS_RAT_ID
  left join bv.Deal dea on dea.DEA_ID = [TrafficData].[PBS].[vwMOSEpisode].DealID
  left join bv.SYSLOOKUP ARLookup on ARLookup.SLU_ID = dea.DEA_SLU_ID_ASPECTRATIO
  where EXT_SEXTC_ID = 607871753 -- RightslineTemplate Column
  and ulkup.ULU_CLASS = 'L03' and ULU_ID != 609587740 
  and charindex('Season',[Episode.ParentID]) &gt;= 1





