              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="ADO NET Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select [ParentID],ChildID
from
(
SELECT DISTINCT
        BV.DEAL.DEA_ID AS [ParentID] ,
        BV.DEALFUNDING.DEF_DEA_ID AS [ChildID] 
FROM    BV.DEALFUNDING
        RIGHT OUTER JOIN BV.DEAL ON BV.DEALFUNDING.DEF_DEA_ID = BV.DEAL.DEA_ID
        LEFT OUTER JOIN BV.MASTERDEAL ON BV.DEAL.DEA_MDL_ID = BV.MASTERDEAL.MDL_ID
        LEFT OUTER JOIN BV.ATTRIBUTE
        RIGHT OUTER JOIN BV.DEAL_ATTRIBUTE ON BV.ATTRIBUTE.ATT_ID = BV.DEAL_ATTRIBUTE.ATT_ID ON BV.DEAL.DEA_ID = BV.DEAL_ATTRIBUTE.DEA_ID
        RIGHT OUTER JOIN BV.DEALCONTENT ON BV.DEAL.DEA_ID = BV.DEALCONTENT.DCO_DEA_ID
        RIGHT OUTER JOIN BV.ASSETRIGHTWINDOW ON BV.DEALCONTENT.DCO_ID = BV.ASSETRIGHTWINDOW.ARW_DCO_ID
        RIGHT OUTER JOIN BV.PBSFEEDDISTRIBUTOR ON BV.DEAL.DEA_PBSFD_ID = BV.PBSFEEDDISTRIBUTOR.PBSFDS_ID
WHERE   ( BV.PBSFEEDDISTRIBUTOR.PBSFDS_ISPBSDISTRIBUTOR &lt;&gt; 0 )
        AND ( BV.DEALCONTENT.DCO_CHA_ID = 5000 )
        AND ( BV.ASSETRIGHTWINDOW.ARW_ARR_ID = 152 )
        OR ( BV.PBSFEEDDISTRIBUTOR.PBSFDS_ISPBSDISTRIBUTOR &lt;&gt; 0 )
        AND ( BV.DEALCONTENT.DCO_CHA_ID = 5000 )
        AND ( BV.ASSETRIGHTWINDOW.ARW_DATEAVAILABLETO &gt;= CONVERT(DATETIME, '2017-12-31 00:00:00', 102) )
) q 
join [RightsLineETL-EA].dbo.Apr23_Deals z on z.dea_id = q.parentid
where [ChildID] is not null</property>
