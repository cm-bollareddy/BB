                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
       cast( 'cid:org:pbs.org:Offer' + cast(BV.DEAL.DEA_ID as varchar(30)) as nvarchar(35)) AS ParentID ,
        BV.ASSET.ASS_ID AS ChildID
FROM    BV.ASSET
        LEFT OUTER JOIN BV.EXTENDEDDATA ON BV.ASSET.ASS_ID = BV.EXTENDEDDATA.EXT_OWNERRECORD_ID
        RIGHT OUTER JOIN BV.DEALCONTENT ON BV.ASSET.ASS_ID = BV.DEALCONTENT.DCO_ASS_ID
        LEFT OUTER JOIN BV.MASTERDEAL
        RIGHT OUTER JOIN BV.DEAL ON BV.MASTERDEAL.MDL_ID = BV.DEAL.DEA_MDL_ID
        LEFT OUTER JOIN BV.ATTRIBUTE
        RIGHT OUTER JOIN BV.DEAL_ATTRIBUTE ON BV.ATTRIBUTE.ATT_ID = BV.DEAL_ATTRIBUTE.ATT_ID ON BV.DEAL.DEA_ID = BV.DEAL_ATTRIBUTE.DEA_ID ON BV.DEALCONTENT.DCO_DEA_ID = BV.DEAL.DEA_ID
        RIGHT OUTER JOIN BV.ASSETRIGHTWINDOW ON BV.DEALCONTENT.DCO_ID = BV.ASSETRIGHTWINDOW.ARW_DCO_ID
        RIGHT OUTER JOIN BV.PBSFEEDDISTRIBUTOR ON BV.DEAL.DEA_PBSFD_ID = BV.PBSFEEDDISTRIBUTOR.PBSFDS_ID
WHERE   ( BV.EXTENDEDDATA.EXT_INTEGER IN ( 609587737, 619411205 ) )
        AND ( BV.PBSFEEDDISTRIBUTOR.PBSFDS_ISPBSDISTRIBUTOR &lt;&gt; 0 )
        AND ( BV.DEALCONTENT.DCO_CHA_ID = 5000 )
        AND ( BV.ASSETRIGHTWINDOW.ARW_ARR_ID = 152 )
        OR ( BV.EXTENDEDDATA.EXT_INTEGER IN ( 609587737, 619411205 ) )
        AND ( BV.PBSFEEDDISTRIBUTOR.PBSFDS_ISPBSDISTRIBUTOR &lt;&gt; 0 )
        AND ( BV.DEALCONTENT.DCO_CHA_ID = 5000 )
        AND ( BV.ASSETRIGHTWINDOW.ARW_DATEAVAILABLETO &gt;= CONVERT(DATETIME, '2017-12-31 00:00:00', 102) )

