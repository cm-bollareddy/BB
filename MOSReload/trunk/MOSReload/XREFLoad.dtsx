                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  distinct
      REVERSE(SUBSTRING(REVERSE([Usage.UID]),0,CHARINDEX(':',REVERSE([Usage.UID])))) as ChildID
	  ,REVERSE(SUBSTRING(REVERSE([Usage.ParentID]),0,CHARINDEX(':',REVERSE([Usage.ParentID])))) as ParentID
  FROM [PBS].[vwMOSUsage]
  join [rightslineETL].[dbo].Apr23_deals Apr23 on  Apr23.OfferUID = substring(vwMOSUsage.[Usage.ParentID],len('parentid:org:pbs.org:')+1,len(vwMOSUsage.[Usage.ParentID]))
  where [Usage.EffectiveTerm.Use] = 'HOLDBACK'


