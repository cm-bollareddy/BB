FROM [PBS].[vwMOSOffer]
Join [RightsLineETL].[dbo].Apr23_Episodes feb28 on  feb28.ASS_ID = [PBS].[vwMOSOffer].ass_id
where [Offer.Entry.UID] like '%EPISODE%'
